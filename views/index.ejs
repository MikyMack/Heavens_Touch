	<script>
	(function() {
		var music = document.getElementById('background-music');
		music.volume = 0.1;

		// Try to play immediately for fastest possible playback (including before DOMContentLoaded)
		function tryPlayMusic() {
			// If the browser blocks autoplay, we fallback to user gesture
			music.play().catch(function() {
				var played = false;
				function unlockOnInteraction() {
					if (played) return;
					music.play().catch(function(){});
					played = true;
					document.removeEventListener('click', unlockOnInteraction);
					document.removeEventListener('touchstart', unlockOnInteraction);
				}
				document.addEventListener('click', unlockOnInteraction, { once: true });
				document.addEventListener('touchstart', unlockOnInteraction, { once: true });
			});
		}

		// Attempt to play as soon as possible, covering all browser behaviors
		tryPlayMusic();
		// Some browsers will fully enable media after DOM ready/load, so try again there
		document.addEventListener('DOMContentLoaded', tryPlayMusic, { once: true });
		window.addEventListener('load', tryPlayMusic, { once: true });
	})();
	</script>
