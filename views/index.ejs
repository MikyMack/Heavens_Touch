<script>
document.addEventListener("DOMContentLoaded", function() {
  const popup = document.getElementById("music-popup");
  const playBtn = document.getElementById("play-music-btn");
  const noBtn = document.getElementById("no-music-btn");
  const music = document.getElementById("background-music");

  // If choice exists, skip popup
  const savedChoice = localStorage.getItem("musicPreference");
  if(savedChoice === "yes") {
    music.play();
    popup.style.display = "none";
  } else if(savedChoice === "no") {
    popup.style.display = "none";
  }

  playBtn.addEventListener("click", () => {
    music.volume = 0.5;
    music.play();
    popup.style.display = "none";
    localStorage.setItem("musicPreference", "yes");
  });

  noBtn.addEventListener("click", () => {
    popup.style.display = "none";
    localStorage.setItem("musicPreference", "no");
  });
});
</script>





