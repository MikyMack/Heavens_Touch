	  <script>
		(function() {
		  // ---------Hero Slide Logic (keep as is)-------------
		  var slides = document.querySelectorAll('.hero-slide-content');
		  var dots = document.querySelectorAll('.hero-dot');
		  var current = 0;
		  var timer;
		  function showSlide(idx) {
			slides.forEach(function(s, i) {
			  s.classList.toggle('hide', i !== idx);
			});
			dots.forEach(function(d, i) {
			  d.classList.toggle('active', i === idx);
			});
			current = idx;
			runTypeWriter(slides[idx]);
		  }
		  function nextSlide() {
			var idx = (current + 1) % slides.length;
			showSlide(idx);
		  }
		  dots.forEach(function(dot, idx) {
			dot.addEventListener('click', function(e) {
			  e.stopPropagation();
			  showSlide(idx);
			  resetAuto();
			});
		  });
		  function startAuto() {
			timer = setInterval(nextSlide, 5000);
		  }
		  function resetAuto() {
			clearInterval(timer);
			startAuto();
		  }
		  // ---------Typewriter Functionality Begins-------------
		  // Safeguard: stop multiple typings on quick slide switches
		  let lastTypeIndex = 0;
		  function runTypeWriter(slide) {
			var h1 = slide.querySelector('.typewriter-h1');
			if(!h1) return;
			const allH1s = document.querySelectorAll('.typewriter-h1');
			allH1s.forEach(function(el) {
			  el.innerHTML = ""; // Clear other slides
			});
			let typeIndex = ++lastTypeIndex;
			var dataStr = h1.getAttribute('data-strings');
			var strings = [];
			try {
			  strings = JSON.parse(dataStr.replace(/'/g,'"'));
			} catch(e) {
			  strings = [h1.textContent || ""];
			}
			var full = strings[0] ? strings[0] : "";
			// Remove HTML tags for typing logic, but re-insert them inline
			// We'll use a but more robust approach supporting <br>
			function typeOut(str, idx, cb) {
			  let i = 0, html = "", openTag = false;
			  function _type() {
				if(typeIndex !== lastTypeIndex) return; // Aborted because slide changed
				let char = str[i];
				if(!char) {
				  html = str.slice(0,i);
				  h1.innerHTML = html + '<span class="typewriter-cursor">&#9608;</span>';
				  setTimeout(function(){
					if(typeIndex === lastTypeIndex) {
					  h1.innerHTML = html; // remove cursor at end
					}
				  }, 600);
				  return cb && cb();
				}
				if(char === "<") {
				  // Copy tag completely
				  let tagEnd = str.indexOf(">", i);
				  if(tagEnd !== -1) {
					html = str.slice(0, tagEnd+1);
					i = tagEnd+1;
				  }
				} else {
				  html = str.slice(0, i+1);
				  i++;
				}
				h1.innerHTML = html + '<span class="typewriter-cursor">&#9608;</span>';
				let delay = (char === "<") ? 0 : (24 + Math.random()*65);
				setTimeout(_type, delay);
			  }
			  _type();
			}
			typeOut(full, 0);
		  }
		  showSlide(0);
		  startAuto();
		  function resizeHero() {
			var hero = document.querySelector('.hero-one');
			var video = document.querySelector('.hero-bg-video');
			var vh = window.innerHeight;
			var vw = window.innerWidth;
			hero.style.height = vh + 'px';
			video.style.height = vh + 'px';
			video.style.width = vw + 'px';
		  }
		  window.addEventListener('resize', resizeHero, {passive:true});
		  document.addEventListener('DOMContentLoaded', function(){
			resizeHero();
			// Also trigger typewriter for slide 0 on DOM ready
			runTypeWriter(slides[0]);
		  });
		  resizeHero();
		})();
	  </script>
