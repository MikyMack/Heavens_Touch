	        <%
	       
	          function formatDateDMY(isoDateStr) {
	            if (!isoDateStr) return "";
	            const dateParts = isoDateStr.split("/");
	      
	            let day, month, year;
	            if (dateParts.length === 3 && isoDateStr.includes("/")) {
	        
	              day = Number(dateParts[0]);
	              month = Number(dateParts[1]) - 1;
	              year = Number(dateParts[2]);
	            } else if (isoDateStr.includes("-")) {
	      
	              const arr = isoDateStr.split("-");
	              year = Number(arr[0]);
	              month = Number(arr[1]) - 1;
	              day = Number(arr[2]);
	            } else {
	              return isoDateStr;
	            }
	            const d = new Date(year, month, day);

	            return d.toLocaleDateString('en-US', {
	              weekday: 'long',
	              year: 'numeric',
	              month: 'long',
	              day: '2-digit'
	            });
	          }
	        %>
	        <div class="bible-page-content-inner" style="padding:32px 24px; text-align:center;">
