	<script src="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.js"></script>
	<script>
	// Initialize Swiper for testimonies only if the container exists
	document.addEventListener('DOMContentLoaded', function() {
		const swiperContainer = document.querySelector('.testimony-swiper');
		if (swiperContainer) {
			new Swiper('.testimony-swiper', {
				slidesPerView: 3,
				spaceBetween: 24,
				centeredSlides: false,
				loop: true,
				autoplay: { delay: 7000, disableOnInteraction: false },
				navigation: {
					nextEl: '.swiper-button-next',
					prevEl: '.swiper-button-prev',
				},
				pagination: {
					el: '.swiper-pagination',
					clickable: true,
				},
				breakpoints: {
					0: { slidesPerView: 1, spaceBetween: 6 },
					576: { slidesPerView: 1, spaceBetween: 6 },
					768: { slidesPerView: 2, spaceBetween: 15 },
					992: { slidesPerView: 3, spaceBetween: 24 },
				}
			});
		}

		const writeBtn = document.getElementById('writeTestimonyBtn');
		const modalEl = document.getElementById('testimonyModal');
		const formEl = document.getElementById('testimonyForm');
		if (writeBtn && modalEl && formEl) {
			writeBtn.addEventListener('click', function() {
				var modal = new bootstrap.Modal(modalEl);
				formEl.reset();
				modal.show();
			});

			formEl.addEventListener('submit', async function(e) {
				e.preventDefault();
				const name = document.getElementById('testimonyName').value.trim();
				const phone = document.getElementById('testimonyPhone').value.trim();
				const testimony = document.getElementById('testimonyText').value.trim();
				if (!name || !phone || !testimony) return;

				// Match Express' .body property for phoneNumber (views input field = name="phoneNumber")
				const payload = { 
					name, 
					phoneNumber: phone, 
					message: testimony 
				};

				const submitBtn = this.querySelector('button[type="submit"]');
				const prevBtnText = submitBtn.innerHTML;
				submitBtn.disabled = true;
				submitBtn.innerHTML = 'Submitting...';

				try {
					const res = await fetch('/createTestimonial', {
						method: 'POST',
						headers: { 'Content-Type': 'application/json' },
						body: JSON.stringify(payload)
					});
					const result = await res.json();

					if (!result.success) {
						alert('Could not submit your testimony. Please try again!');
						submitBtn.disabled = false;
						submitBtn.innerHTML = prevBtnText;
						return;
					}

					location.reload();
				} catch(err) {
					alert('Could not submit your testimony. Please try again!');
				} finally {
					submitBtn.disabled = false;
					submitBtn.innerHTML = prevBtnText;
				}
			});
		}
	});
