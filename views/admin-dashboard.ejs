                            <!-- Inline Script for All Logic (Create, Edit, Delete) with Fetch API -->
                            <script>
                              // Detect if Bootstrap 5 modal JS is present; if not, load dynamically (for dashboard:352 error)
                              function loadBootstrapModalIfNeeded(cb) {
                                if(typeof window.bootstrap === "undefined" && typeof window.jQuery !== "undefined") {
                                  var script = document.createElement("script");
                                  script.src = "https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js";
                                  script.onload = cb || function(){};
                                  document.head.appendChild(script);
                                } else if(cb) { cb(); }
                              }

                              // Helper: Get YouTube Id
                              function getYtId(url){
                                var regExp = /(?:youtube\.com.*(?:\\?|&)v=|youtu\.be\/)([^&#\s]+)/;
                                var match = url.match(regExp);
                                return match && match[1] ? match[1] : null;
                              }

                              function ensureModal(cb) {
                                if(typeof window.bootstrap === "undefined") {
                                  loadBootstrapModalIfNeeded(cb);
                                } else {
                                  cb();
                                }
                              }

                              document.addEventListener("DOMContentLoaded", function() {

                                var videoModal, videoModalTitle, videoModalForm, editVideoId, modalVideoTitle, modalVideoLink;

                                function initElements() {
                                  videoModalTitle = document.getElementById('videoModalLabel');
                                  videoModalForm = document.getElementById('videoModalForm');
                                  editVideoId = document.getElementById('editVideoId');
                                  modalVideoTitle = document.getElementById('modalVideoTitle');
                                  modalVideoLink = document.getElementById('modalVideoLink');
                                }

                                function getModalInstance(){
                                  // Bootstrap 5 mode
                                  if (typeof bootstrap !== "undefined" && bootstrap.Modal) {
                                    return new bootstrap.Modal(document.getElementById('videoModal'), {});
                                  } 
                                  return null;
                                }

                                function openModal() {
                                  videoModalTitle.textContent = modalVideoFormMode === 'add' ? 'Add Video' : 'Edit Video';
                                  if (!window.videoModalInstance) {
                                    window.videoModalInstance = getModalInstance();
                                  }
                                  if (window.videoModalInstance) window.videoModalInstance.show();
                                  else document.getElementById("videoModal").style.display="block";
                                }
                                var modalVideoFormMode = 'add';

                                // Add
                                document.getElementById('showAddModalBtn').addEventListener('click', function() {
                                  ensureModal(function() {
                                    initElements();
                                    modalVideoFormMode = 'add';
                                    videoModalTitle.textContent = 'Add Video';
                                    modalVideoTitle.value = '';
                                    modalVideoLink.value = '';
                                    editVideoId.value = '';
                                    openModal();
                                  });
                                });

                                // Edit
                                document.querySelectorAll('.editVideoBtn').forEach(function(btn) {
                                  btn.addEventListener('click', function() {
                                    ensureModal(function() {
                                      initElements();
                                      const card = btn.closest('.category-card');
                                      const id = card.dataset.id;
                                      const title = card.querySelector('.category-title').textContent.trim();
                                      const ytBtn = card.querySelector('a.btn-outline-primary');
                                      const link = ytBtn ? ytBtn.getAttribute('href') : '';
                                      modalVideoFormMode = 'edit';
                                      videoModalTitle.textContent = 'Edit Video';
                                      modalVideoTitle.value = title;
                                      modalVideoLink.value = link;
                                      editVideoId.value = id;
                                      openModal();
                                    });
                                  });
                                });

                                // Handle Create/Edit (AJAX with fetch)
                                document.getElementById('videoModalForm').addEventListener('submit', async function(e) {
                                  e.preventDefault();
                                  const id = document.getElementById('editVideoId').value;
                                  const title = document.getElementById('modalVideoTitle').value.trim();
                                  const link = document.getElementById('modalVideoLink').value.trim();

                                  const submitBtn = this.querySelector('button[type="submit"]');
                                  submitBtn.disabled = true;

                                  if (!title || !link) { alert("Both Title and Link required"); submitBtn.disabled = false; return; }
                                  if (!getYtId(link)) { alert("Please enter a valid YouTube link."); submitBtn.disabled = false; return; }

                                  if (!id) {
                                    // CREATE
                                    const formData = new URLSearchParams();
                                    formData.append('title', title);
                                    formData.append('link', link);
                                    try {
                                      const resp = await fetch('/admin/videos', {
                                        method: 'POST',
                                        headers: { 'Content-Type': 'application/x-www-form-urlencoded' },
                                        body: formData
                                      });
                                      if (resp.redirected || resp.status === 200) {
                                        location.reload();
                                      } else {
                                        alert("Could not create video!");
                                        submitBtn.disabled = false;
                                      }
                                    } catch (err) {
                                      alert("Network error.");
                                      submitBtn.disabled = false;
                                    }
                                  } else {
                                    // EDIT (simulate PUT with fetch)
                                    // Note: You must have a matching backend route /admin/videos/:id with PUT method!
                                    const formData = new URLSearchParams();
                                    formData.append('title', title);
                                    formData.append('link', link);
                                    try {
                                      const resp = await fetch(`/admin/videos/${id}?_method=PUT`, {
                                        method: 'POST',
                                        headers: { 'Content-Type': 'application/x-www-form-urlencoded' },
                                        body: formData
                                      });
                                      if (resp.redirected || resp.status === 200) {
                                        location.reload();
                                      } else {
                                        alert("Could not update video!");
                                        submitBtn.disabled = false;
                                      }
                                    } catch (err) {
                                      alert("Network error.");
                                      submitBtn.disabled = false;
                                    }
                                  }
                                });

                                // Handle Delete
                                document.querySelectorAll('.deleteVideoBtn').forEach(function(btn) {
                                  btn.addEventListener('click', async function() {
                                    if(!confirm('Are you sure you want to delete this video?')) return;
                                    const card = btn.closest('.category-card');
                                    const id = card.dataset.id;
                                    // Show loading
                                    const overlay = card.querySelector('.spinner-overlay');
                                    overlay.classList.remove('d-none');
                                    try {
                                      const resp = await fetch(`/admin/videos/delete/${id}`, {
                                        method: 'GET'
                                      });
                                      if (resp.redirected || resp.status === 200) {
                                        // Fade out card
                                        card.style.transition = 'opacity .3s';
                                        card.style.opacity = 0.3;
                                        setTimeout(() => { 
                                          card.remove(); 
                                          // If no cards left, show "No videos" - one less reload!
                                          if (!document.querySelectorAll('.category-card').length) {
                                            document.getElementById('videoGrid').innerHTML = '<div class="w-100 text-center text-muted py-4">No videos added yet.</div>';
                                          }
                                        }, 400);
                                      } else {
                                        alert('Could not delete!');
                                      }
                                    } catch (e) {
                                      alert('Network error!');
                                    } finally {
                                      overlay.classList.add('d-none');
                                    }
                                  });
                                });
                              });
                            </script>
                            <style>
                              .category-grid {
                                display: grid;
                                grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));
                                gap: 22px;
                                margin-top: 20px;
                              }
                              .category-card {
                                border: 1px solid #e0e0e0;
                                border-radius: 8px;
                                overflow: hidden;
                                box-shadow: 0 3px 10px rgba(0,0,0,0.07);
                                transition: transform 0.2s, box-shadow 0.2s;
                                background: #fff;
                                min-height: 280px;
                                position:relative;
                              }
                              .category-card:hover {
                                transform: translateY(-3px) scale(1.01);
                                box-shadow: 0 8px 24px rgba(236,33,39,0.14);
                              }
                              .category-image {
                                height: 150px;
                                background: #19191912;
                                overflow: hidden;
                              }
                              .category-title {
                                margin: 0 0 10px 0;
                                font-size: 1.1rem;
                                color: #c4302b;
                                font-weight:600;
                              }
                              .category-body {
                                padding: 18px 15px 15px 15px;
                                display: flex;
                                flex-direction: column;
                              }
                              .spinner-overlay {
                                display: none;
                              }
                              .category-card .spinner-overlay:not(.d-none) {
                                display:flex!important;
                              }
                              @media (max-width: 768px) {
                                .main-content-wrap {
                                  padding: 0.5rem;
                                }
                                .category-grid {
                                  grid-template-columns: 1fr;
                                }
                              }
                              #showAddModalBtn {
                                font-size: 1.1rem;
                                font-weight: 500;
                                border-radius: 40px;
                                letter-spacing:0.02em;
                              }
                              .editVideoBtn,.deleteVideoBtn {
                                min-width: 44px;
