                                <!-- Videos List Grid -->
                                <div class="category-grid" id="videoGrid">
                                  <% if (youtubeVideos && youtubeVideos.length) { %>
                                    <% youtubeVideos.forEach(function(video, idx) { %>
                                      <div class="category-card position-relative" data-id="<%= video._id %>">
                                        <div class="category-image">
                                          <% 
                                            function getYtId(url){
                                              var regExp = /(?:youtube\.com.*(?:\\?|&)v=|youtu\.be\/)([^&#\s]+)/;
                                              var match = url.match(regExp);
                                              return match && match[1] ? match[1] : null;
                                            }
                                            var vId = getYtId(video.link);
                                          %>
                                          <% if (vId) { %>
                                            <iframe width="100%" height="150" src="https://www.youtube.com/embed/<%= vId %>" frameborder="0" allowfullscreen style="border-radius:0 0 4px 4px;"></iframe>
                                          <% } else { %>
                                            <div class="bg-light d-flex align-items-center justify-content-center" style="height:100%;">
                                              <span class="text-danger small">Invalid Link</span>
                                            </div>
                                          <% } %>
                                        </div>
                                        <div class="category-body">
                                          <h3 class="category-title text-truncate" title="<%= video.title %>"><%= video.title %></h3>
                                          <a href="<%= video.link %>" class="btn btn-outline-primary btn-sm mb-2" target="_blank">
                                            <i class="fa fa-play"></i> Watch
                                          </a>
                                          <div class="text-muted small mb-2">
                                            Added: <%= new Date(video.createdAt).toLocaleDateString() %>
                                          </div>
                                          <div class="d-flex gap-2">
                                            <button class="btn btn-warning btn-sm flex-fill editVideoBtn"><i class="fa fa-pencil"></i> Edit</button>
                                            <button class="btn btn-danger btn-sm flex-fill deleteVideoBtn"><i class="fa fa-trash"></i> Delete</button>
                                          </div>
                                        </div>
                                        <div class="spinner-overlay position-absolute top-0 start-0 w-100 h-100 d-none"
                                             style="background:rgba(255,255,255,0.7);z-index:5;display:flex;justify-content:center;align-items:center;">
                                          <div class="spinner-border text-danger"></div>
                                        </div>
                                      </div>
                                    <% }); %>
                                  <% } else { %>
                                    <div class="w-100 text-center text-muted py-4">
                                      No videos added yet.
                                    </div>
                                  <% } %>
